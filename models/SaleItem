'use strict';
const mongoose = require( 'mongoose' );
const Schema = mongoose.Schema;
const ObjectId = Schema.Types.ObjectId;

var toDoItemSchema = Schema( {
  userId: ObjectId,
  itemTitle: String,
  price: Number,
  description:String,
  createdAt: Date,
} );

module.exports = mongoose.model( 'SaleItem', toDoItemSchema );
